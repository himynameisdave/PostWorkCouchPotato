{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "camelcase": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "react/forbid-prop-types": 0
  }
}
